<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.victor.myintro">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity android:name="com.example.victor.myintro.MainActivity"
            android:label="Welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name="com.example.victor.myintro.MyIntro"
            android:label="My trial App">
            <intent-filter>
                <action android:name="android.intent.action.MAIN01" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>


        <!--
        <activity
            android:name=".MainActivity"
            android:label="@string/title_activity_my_intro"
            android:theme="@style/AppTheme.NoActionBar" />
            -->

    </application>

</manifest>